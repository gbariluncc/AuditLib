    <Style x:Key="Minimizer" TargetType="{x:Type Button}">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Foreground" Value="Orange" />
        <Setter Property="Height" Value="50" />
        <Setter Property="Width" Value="50" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Margin="-0.8,0" Height="50" Width="50">
                        <Border Height="50" Width="50" BorderBrush="Transparent" BorderThickness="1" HorizontalAlignment="Left"  VerticalAlignment="Top" CornerRadius="33">
                            <Border.Style>
                                <Style>
                                    <Style.Triggers>
                                        <Trigger Property="Border.IsMouseOver" Value="True">
                                            <Setter Property="Border.Background" Value="LightGray" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <Path Data="{StaticResource Minus}" 
                                  Fill="{Binding Foreground, 
                                 RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}" 
                                 Stretch="Uniform" Height="5" Width="15" />
                        </Border>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="CornflowerBlue" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="Expander" TargetType="{x:Type Button}">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Margin="-0.8,0">
                        <Border BorderBrush="Transparent" BorderThickness="1" HorizontalAlignment="Left"  VerticalAlignment="Top" CornerRadius="33">
                            <Border.Style>
                                <Style>
                                    <Style.Triggers>
                                        <Trigger Property="Border.IsMouseOver" Value="True">
                                            <Setter Property="Border.Background" Value="LightGray" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <Path Data="{StaticResource Plus}"
                              Stretch="Uniform"
                              Margin="15"
                              Fill="{Binding Foreground, 
                              RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}"/>
                        </Border>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="Document" TargetType="{x:Type Button}">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Content">
            <Setter.Value>
                <Canvas>
                    <Path Data="{StaticResource PaperClip}" Canvas.Top="-15" Canvas.Left="-15" Stretch="Uniform" Fill="Black" />
                </Canvas>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RoundButton" TargetType="{x:Type Button}">
        <Grid Margin="-0.8,0">
            <Border BorderBrush="Transparent" BorderThickness="1" HorizontalAlignment="Left"  VerticalAlignment="Top" CornerRadius="33"
                    Padding="10">
                <Border.Style>
                    <Style>
                        <Style.Triggers>
                            <Trigger Property="Border.IsMouseOver" Value="True">
                                <Setter Property="Border.Background" Value="LightGray" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Border.Style>
                <StackPanel>
                    <Path Data="{StaticResource Menu}" Fill="Black" VerticalAlignment="Center" Margin="3" Stretch="Uniform" />
                </StackPanel>
            </Border>
            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="RoundNext" TargetType="{x:Type Button}">
        <Grid Margin="-0.8,0">
            <Border BorderBrush="Black" x:Name="ButtonBorder" BorderThickness="2" HorizontalAlignment="Left" CornerRadius="33">
                <Border.Style>
                    <Style>
                        <Style.Triggers>
                            <Trigger Property="Border.IsMouseOver" Value="True">
                                <Setter Property="Border.Background" Value="LightGray" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Border.Style>
                <Path Data="{StaticResource Next}"
                      x:Name="Arrow"
                      Fill="{Binding Foreground, 
                      RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}" 
                      Stretch="Uniform"
                      Margin="5,9"
                      >
                </Path>
            </Border>
            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="UserControl.IsEnabled" Value="False">
                <Setter TargetName="ButtonBorder" Property="BorderBrush" Value="LightGray" />
                <Setter TargetName="ButtonBorder" Property="Background" Value="Black" />
                <Setter TargetName="Arrow" Property="Fill" Value="LightGray" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="RoundBack" TargetType="{x:Type Button}">
        <Grid Margin="-0.8,0">
            <Border x:Name="ButtonBorder" BorderBrush="Black" BorderThickness="2" HorizontalAlignment="Left" CornerRadius="33">
                <Border.Style>
                    <Style>
                        <Style.Triggers>
                            <Trigger Property="Border.IsMouseOver" Value="True">
                                <Setter Property="Border.Background" Value="LightGray" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Border.Style>
                <Path Data="{StaticResource Next}"
                      x:Name="Arrow"
                      Fill="{Binding Foreground, 
                      RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}" 
                      Stretch="Uniform"
                      Margin="5,9"
                      >
                </Path>
            </Border>
            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="UserControl.IsEnabled" Value="False">
                <Setter TargetName="ButtonBorder" Property="BorderBrush" Value="Red" />
                <Setter TargetName="Arrow" Property="Fill" Value="DarkGray" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    
